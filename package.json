    "remark-frontmatter": "^5.0.0",
    "remark-html": "^16.0.1",
    "vue": "^3.0.5",
    "vue-safe-html": "^3.0.1",
    "yaml": "^1.10.0"
  },
  "engines": {
    "node": "^18"
  },
  "devDependencies": {
    "@types/page": "^1.11.1",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "concurrently": "^7.2.0",
    "lighthouse": "^9.6.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.1",
    "vite-svg-loader": "^1.4.2"
  }
