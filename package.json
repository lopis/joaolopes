{
  "name": "raven-joy",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "index": "node src/util/index.js",
    "test:lighthouse": "lighthouse http://localhost:5000/ --quiet --chrome-flags=\"--headless\" --output-path=lighthouse-report.html",
